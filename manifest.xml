<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
  xmlns:mailappor="http://schemas.microsoft.com/office/mailappversionoverrides/1.0"
  xsi:type="MailApp">
  <Id>c2c560d2-91ce-4f59-883a-f6fff90e5db2</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>K-Businesscom AG</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="VMS" />
  <Description DefaultValue="Register external visitors" />
  <IconUrl
    DefaultValue="https://christoph-r.github.io/outlook-addin/assets/vms-icon-64.png" />
  <HighResolutionIconUrl
    DefaultValue="https://christoph-r.github.io/outlook-addin/assets/vms-icon-128.png" />
  <SupportUrl DefaultValue="https://www.k-business.com" />
  <AppDomains>
    <AppDomain>
      https://localhost:3000</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Mailbox" />
  </Hosts>
  <Requirements>
    <Sets>
      <Set Name="Mailbox" MinVersion="1.1" />
    </Sets>
  </Requirements>

  <!-- Old schema. This will be overridden by the VersionOverrides below. -->
  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://christoph-r.github.io/outlook-addin/src/commands/commands.html" />
        <RequestedHeight>250</RequestedHeight>
      </DesktopSettings>
    </Form>
  </FormSettings>
  <Permissions>ReadWriteItem</Permissions>
  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" ItemType="Appointment" FormType="Edit" />
  </Rule>
  <DisableEntityHighlighting>false</DisableEntityHighlighting>
  <!-- /Old schema. -->

  <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides"
    xsi:type="VersionOverridesV1_0">
    <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides/1.1"
      xsi:type="VersionOverridesV1_1">
      <Description resid="residAppDesc" />
      <Requirements>
        <bt:Sets DefaultMinVersion="1.3">
          <bt:Set Name="Mailbox" />
        </bt:Sets>
      </Requirements>
      <Hosts>
        <Host xsi:type="MailHost">

          <!-- Specify the runtime for event-based activation. -->
          <!-- For more information, see
          https://learn.microsoft.com/office/dev/add-ins/outlook/autolaunch -->
          <Runtimes>
            <!-- HTML file including reference to or inline JavaScript event handlers. This is used
            by Outlook on the web. -->
            <Runtime resid="WebViewRuntime.Url"> <!-- /src/commands/commands.html -->
              <!-- JavaScript file containing event handlers. This is used by Outlook on Windows. -->
              <Override type="javascript" resid="JSRuntime.Url" />
              <!-- /src/commands/commands.js -->
            </Runtime>
          </Runtimes>

          <DesktopFormFactor>
            <FunctionFile resid="functionFile" />
            <ExtensionPoint xsi:type="AppointmentOrganizerCommandSurface">
              <OfficeTab id="TabDefault">
                <Group id="appOrgGroup">
                  <Label resid="GroupLabel" />
                  <Control xsi:type="Menu" id="appLocationButton">
                    <Label resid="menuLocationListLabel" />
                    <Supertip>
                      <Title resid="menuLocationListLabel" />
                      <Description resid="menuLocationListLabel" />
                    </Supertip>
                    <Icon>
                      <bt:Image size="16" resid="Icon.16" />
                      <bt:Image size="32" resid="Icon.32" />
                      <bt:Image size="80" resid="Icon.80" />
                    </Icon>
                    <Items>
                      <Item id="location1">
                        <Label resid="location1Label" />
                        <Supertip>
                          <Title resid="location1Label" />
                          <Description resid="location1Label" />
                        </Supertip>
                        <Action xsi:type="ExecuteFunction">
                          <FunctionName>addLocation</FunctionName>
                        </Action>
                      </Item>
                      <Item id="location2">
                        <Label resid="location2Label" />
                        <Supertip>
                          <Title resid="location1Label" />
                          <Description resid="location1Label" />
                        </Supertip>
                        <Action xsi:type="ExecuteFunction">
                          <FunctionName>addLocation</FunctionName>
                        </Action>
                      </Item>
                    </Items>
                  </Control>
                </Group>
              </OfficeTab>
            </ExtensionPoint>
          </DesktopFormFactor>
        </Host>
      </Hosts>
      <Resources>
        <bt:Images>
          <bt:Image id="Icon.16"
            DefaultValue="https://christoph-r.github.io/outlook-addin/assets/vms-icon-16.png" />
          <bt:Image id="Icon.32"
            DefaultValue="https://christoph-r.github.io/outlook-addin/assets/vms-icon-32.png" />
          <bt:Image id="Icon.64"
            DefaultValue="https://christoph-r.github.io/outlook-addin/assets/vms-icon-64.png" />
          <bt:Image id="Icon.80"
            DefaultValue="https://christoph-r.github.io/outlook-addin/assets/vms-icon-80.png" />
          <bt:Image id="Icon.128"
            DefaultValue="https://christoph-r.github.io/outlook-addin/assets/vms-icon-128.png" />
        </bt:Images>
        <bt:Urls>
          <bt:Url id="functionFile"
            DefaultValue="https://christoph-r.github.io/outlook-addin/src/commands/commands.html" />
          <bt:Url id="WebViewRuntime.Url"
            DefaultValue="https://christoph-r.github.io/outlook-addin/src/commands/commands.html" />
          <!-- Entry needed for Outlook Desktop. -->
          <bt:Url id="JSRuntime.Url" DefaultValue="https://christoph-r.github.io/outlook-addin/src/commands/commands.js" />
        </bt:Urls>
        <bt:ShortStrings>
          <bt:String id="GroupLabel" DefaultValue="VMS" />
          <bt:String id="menuLocationListLabel" DefaultValue="Locations" />
          <bt:String id="location1Label" DefaultValue="Wienerberg" />
          <bt:String id="location2Label" DefaultValue="Raaba" />
        </bt:ShortStrings>
        <bt:LongStrings>
          <bt:String id="menuLocationListLabel"
            DefaultValue="Show instructions for running the sample" />
          <bt:String id="location1Label" DefaultValue="Show instructions for running the sample" />
          <bt:String id="location2Label" DefaultValue="PnP Event-based Activation Sample."></bt:String>
        </bt:LongStrings>
      </Resources>
    </VersionOverrides>
  </VersionOverrides>
</OfficeApp>
